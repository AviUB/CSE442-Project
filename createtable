CREATE TABLE Foods (
    Calories int,
    Name varchar(255),
    Carbs int,
    Protein int,
    Fats int
);

--The code above creates a table containing the calories, name of food, carbs, proteins and fats

INSERT INTO Foods (Calories, Name, Carbs, Protein, Fats)
   VALUES ('200','Carrots', '40','5','12');

--The code above writes the following information about carrots into the table

SELECT Calories, Name, Carbs  FROM Foods;
--The following code above reads the calories names and carbs of all foods from the table and displays the information..
